 <table class="table table-bordered" id="cus_dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>First Name </th>
            <th>Last Name</th>
            <th>Mobile</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Mobile</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </tfoot>
    <tbody id="cus_contact">
    {% for contact in contacts %}
        <tr>
            <td>{{contact.getFirstName()}}</td>
            <td>{{contact.getLastName()}}</td>
            <td>{{contact.getMobile()}}</td>
            <td>{{contact.getEmail()}}</td>
            <td>
                <a href="{{ path('app_contact_edit',{'id':contact.id})}}" class="btn btn-primary">
                Edit
                </a>
                <a href="{{ path('app_contact_delete',{'id':contact.id})}}" onclick="return confirm('Are you sure?')" class="btn btn-danger">
                Delete
                </a>
                <div class="btn-group" role="group" aria-label="Basic example">
                    <a href="{{ path('app_contact_view',{'id':contact.id})}}" class="btn btn-secondary">View</a>
                </div>
            
            </td>
        </tr>
    {% endfor %}
        
        
    </tbody>
</table>
